@{
    ViewBag.Title = "Home Page";

    if (ViewBag.Mensaje == "Success")
    {
        <script >
            alert('XML Success');
        </script>
    }
    else if(ViewBag.Mensaje != null)
    {
        <script>
            alert('@ViewBag.Mensaje');
        </script>
    }


}

